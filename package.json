{
  "name": "karim-portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "if exist .next rmdir /s /q .next && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "dev:clean": "npm run clean && npm run dev",
    "build:clean": "npm run clean && npm run build",
    "dev:watch": "nodemon --watch src --ext js,jsx,ts,tsx,css --exec 'npm run dev'",
    "pm2:start": "pm2 start npm --name 'karim-portfolio' -- run start",
    "pm2:dev": "pm2 start npm --name 'karim-portfolio-dev' -- run dev",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:delete": "pm2 delete all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "critters": "^0.0.23",
    "firebase": "^11.5.0",
    "firebase-admin": "^13.2.0",
    "framer-motion": "^12.5.0",
    "gsap": "^3.12.7",
    "next": "15.2.3",
    "next-themes": "^0.4.6",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-phone-input-2": "^2.15.1",
    "react-phone-number-input": "^3.4.12",
    "react-type-animation": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "nodemon": "^3.1.9",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
